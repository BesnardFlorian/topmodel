---
app: Jpa
allowCompositePrimaryKey: false
modelRoot: ../../model
lockFileName: jpa.topmodel.lock
i18n:
  rootPath: i18n/{lang}/in
  defaultLang: fr_FR
  langs:
    - en_US
    - de_DE
translation:
  - tags:
      - front
      - back
    outputDirectory: ./
    rootPath: i18n/{lang}/out
    langs:
      - en_US
      - de_DE