---
endpoint:
  name: deleteOrder
  method: DELETE
  route: store/order/{orderId}
  description: "Delete purchase order by ID"
  params:
    - name: OrderId
      domain: DO_ID
      comment: "ID of the order that needs to be deleted"
---
endpoint:
  name: getInventory
  method: GET
  route: store/inventory
