    - name: OrderId
      domain: DO_ID
      comment: "ID of order that needs to be fetched"
  returns:
    composition: Order
    name: Result
    kind: object
    comment: "Result"
---
endpoint:
  name: placeOrder
  method: POST
  route: store/order
  description: "Place an order for a pet"
  params:
    - composition: Order
      name: Body
      kind: object
      comment: "Body"
  returns:
    composition: Order
    name: Result
    kind: object
    comment: "Result"
