{
  "name": "topmodel",
  "displayName": "topmodel",
  "description": "Extension pour TopModel",
  "publisher": "Jabx",
  "contributors": [
    {
      "name": "gideruette"
    },
    {
      "name": "Jabx"
    }
  ],
  "extensionDependencies": [
    "redhat.vscode-yaml"
  ],
  "repository": {
    "url": "https://github.com/JabX/topmodel",
    "directory": "TopModel.VSCode",
    "type": "git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.topmodel",
        "title": "TopModel : Start model generation"
      },
      {
        "command": "extension.topmodel.watch",
        "title": "TopModel : Start model generation (watch mode)"
      }
    ]
  },
  "main": "./out/main.js",
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "esbuild": "^0.14.1",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vsce": "2.5.1"
  },
  "dependencies": {
    "vscode-jsonrpc": "^6.0.0",
    "vscode-languageclient": "^7.0.0"
  }
}
