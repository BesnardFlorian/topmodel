{
  "name": "topmodel",
  "displayName": "topmodel",
  "description": "Extension pour TopModel",
  "publisher": "JabX",
  "contributors": [
    {
      "name": "gideruette"
    },
    {
      "name": "Jabx"
    }
  ],
  "extensionDependencies": [
    "redhat.vscode-yaml"
  ],
  "repository": {
    "url": "https://github.com/klee-contrib/topmodel",
    "directory": "TopModel.VSCode",
    "type": "git"
  },
  "version": "1.2.5",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "workspaceContains:**/topmodel*.config"
  ],
  "contributes": {
    "commands": [
      {
        "command": "topmodel.modgen.install",
        "title": "TopModel : Install TopModel.Generator"
      },
      {
        "command": "topmodel.modgen.update",
        "title": "TopModel : Update TopModel.Generator"
      },
      {
        "command": "topmodel.modgen",
        "title": "TopModel : Start model generation"
      },
      {
        "command": "topmodel.modgen.watch",
        "title": "TopModel : Start model generation (watch mode)"
      }
    ],
    "yamlValidation": [
      {
        "fileMatch": "topmodel*.config",
        "url": "./language-server/schema.config.json"
      },
      {
        "fileMatch": "*.tmd",
        "url": "./language-server/schema.json"
      }
    ],
    "languages": [
      {
        "id": "yaml",
        "extensions": [
          "tmd"
        ],
        "filenamePatterns": [
          "topmodel*.config"
        ]
      }
    ],
    "walkthroughs": [
      {
        "id": "topmodel.configuration",
        "title": "Topmodel configuration",
        "description": "Configuration de topmodel pas Ã  pas",
        "steps": [

          {
            "id": "install.dotnet",
            "title": "Installer Dotnet",
            "media": {
              "markdown": "media/dotnet.md"
            }
          },
          {
            "id": "install.modgen",
            "title": "Installation de TopModel.Generator",
            "description": "Cliquez ici pour lancer la commande d'installation de TopModel.\n[Installer TopModel](command:topmodel.modgen.install)",
            "media": {
              "markdown": "media/modgen.md"
            },
            "completionEvents": [
              "onCommand:topmodel.modgen.install"
            ]
          }
        ]
      }
    ]
  },
  "main": "./out/main.js",
  "scripts": {
    "build": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "lint": "eslint src --ext ts",
    "package": "vsce package",
    "start": "npm run build -- --sourcemap --watch",
    "vscode:prepublish": "npm run build -- --minify"
  },
  "dependencies": {
    "open": "^8.4.0",
    "vscode-jsonrpc": "^6.0.0",
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "esbuild": "^0.14.1",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "typescript": "^4.4.4",
    "vsce": "2.6.3"
  }
}
